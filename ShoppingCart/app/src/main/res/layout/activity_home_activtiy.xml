        android:text="@string/ready_to_ad"
        android:textAppearance="@style/ready_to_ad"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/ellipse_3"
        app:layout_constraintHorizontal_bias="0.565"
        app:layout_constraintStart_toStartOf="@+id/ezkart"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.199" />

    <TextView
        android:id="@+id/help"
        android:layout_width="31dp"
        android:layout_height="17dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:gravity="top"
        android:text="@string/help"
        android:textAppearance="@style/help"
        app:layout_constraintBottom_toBottomOf="@+id/connectbutt3"
        app:layout_constraintEnd_toEndOf="@+id/connectbutt3"
        app:layout_constraintHorizontal_bias="0.508"
        app:layout_constraintStart_toStartOf="@+id/connectbutt3"
        app:layout_constraintTop_toTopOf="@+id/connectbutt3"
        app:layout_constraintVertical_bias="0.771" />

    <TextView
        android:id="@+id/orders"
        android:layout_width="46dp"
        android:layout_height="17dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:gravity="top"
        android:text="@string/orders"
        android:textAppearance="@style/orders"
        app:layout_constraintBottom_toBottomOf="@+id/previousord"
        app:layout_constraintEnd_toEndOf="@+id/previousord"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="@+id/previousord"
        app:layout_constraintTop_toTopOf="@+id/previousord"
        app:layout_constraintVertical_bias="0.891" />

    <RelativeLayout
        android:id="@+id/homebar"
        android:layout_width="410dp"
        android:layout_height="80dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/homebar_ek1"
        app:layout_constraintStart_toStartOf="@+id/homebar_ek1"
        app:layout_constraintTop_toTopOf="@+id/homebar_ek1"><![CDATA[>

        ]]></RelativeLayout>

    <View
        android:id="@+id/homebar_ek1"
        android:layout_width="0dp"
        android:layout_height="75dp"
        android:background="@drawable/homebar"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/connectbutt3"
        app:layout_constraintVertical_bias="1.0" />


    <ImageView
        android:id="@+id/home"
        android:layout_width="31.77dp"
        android:layout_height="30dp"
        android:contentDescription="vector_ek6"
        android:src="@drawable/home"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/homebar_ek1"
        app:layout_constraintHorizontal_bias="0.121"
        app:layout_constraintStart_toStartOf="@+id/homebar_ek1"
        app:layout_constraintTop_toTopOf="@+id/homebar_ek1"
        app:layout_constraintVertical_bias="0.644" />



    <ImageView
        android:id="@+id/cart"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:contentDescription="vector_ek7"
        android:src="@drawable/cart"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.9"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/homebar_ek1"
        app:layout_constraintVertical_bias="0.644" />

    <ImageView
        android:id="@+id/inputscan"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/connectbutt3"
        app:srcCompat="@drawable/ic_scan1" />




