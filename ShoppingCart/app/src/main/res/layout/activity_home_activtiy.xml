<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    tools:context=".MainActivity">

    <View
        android:id="@+id/connectocart"
        android:layout_width="275dp"
        android:layout_height="100dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="8dp"
        android:background="@drawable/connectbutt"
        app:layout_constraintEnd_toEndOf="@+id/ready_to_ad"
        app:layout_constraintHorizontal_bias="0.333"
        app:layout_constraintStart_toStartOf="@+id/ready_to_ad"
        app:layout_constraintTop_toBottomOf="@+id/ready_to_ad" />

    <View
        android:id="@+id/previousord"
        android:layout_width="275dp"
        android:layout_height="100dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginBottom="16dp"
        android:background="@drawable/connectbutt"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/connectocart"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/connectocart"
        app:layout_constraintTop_toBottomOf="@+id/connectocart"
        app:layout_constraintVertical_bias="0.19" />

    <View
        android:id="@+id/group_19"
        android:layout_width="34dp"
        android:layout_height="32dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:background="#00000000"
        android:foreground="@drawable/ic_baseline_shopping_basket_24"
        android:foregroundTint="#040000"
        app:layout_constraintBottom_toBottomOf="@+id/connectocart"
        app:layout_constraintEnd_toEndOf="@+id/connectocart"
        app:layout_constraintStart_toStartOf="@+id/connectocart"
        app:layout_constraintTop_toTopOf="@+id/connectocart"
        app:layout_constraintVertical_bias="0.25" />

    <TextView
        android:id="@+id/connect_to_"
        android:layout_width="117dp"
        android:layout_height="17dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:gravity="top"
        android:text="@string/connect_to_"
        android:textAppearance="@style/connect_to_"
        app:layout_constraintBottom_toBottomOf="@+id/connectocart"
        app:layout_constraintEnd_toEndOf="@+id/connectocart"
        app:layout_constraintHorizontal_bias="0.57"
        app:layout_constraintStart_toStartOf="@+id/connectocart"
        app:layout_constraintTop_toBottomOf="@+id/group_19" />

    <TextView
        android:id="@+id/check_your_"
        android:layout_width="233dp"
        android:layout_height="19dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:gravity="top"
        android:text="@string/check_your_"
        android:textAppearance="@style/check_your_"
        app:layout_constraintBottom_toTopOf="@+id/previousord"
        app:layout_constraintEnd_toEndOf="@+id/connectocart"
        app:layout_constraintStart_toStartOf="@+id/connectocart"
        app:layout_constraintTop_toBottomOf="@+id/connectocart" />

    <View
        android:id="@+id/connectbutt3"
        android:layout_width="275dp"
        android:layout_height="100dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="60dp"
        android:background="@drawable/connectbutt"
        app:layout_constraintEnd_toEndOf="@+id/previousord"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/previousord"
        app:layout_constraintTop_toBottomOf="@+id/previousord" />

    <View
        android:id="@+id/group_20"
        android:layout_width="34dp"
        android:layout_height="32dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:background="#00000000"

        android:foreground="@drawable/ic_baseline_list_alt_24"
        app:layout_constraintBottom_toTopOf="@+id/orders"
        app:layout_constraintEnd_toEndOf="@+id/previousord"
        app:layout_constraintHorizontal_bias="0.502"
        app:layout_constraintStart_toStartOf="@+id/previousord"
        app:layout_constraintTop_toTopOf="@+id/previousord"
        app:layout_constraintVertical_bias="0.642" />

    <View
        android:id="@+id/group_21"
        android:layout_width="34dp"
        android:layout_height="32dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:background="#00000000"

        android:foreground="@drawable/ic_baseline_help_center_24"
        app:layout_constraintBottom_toTopOf="@+id/help"
        app:layout_constraintEnd_toEndOf="@+id/connectbutt3"
        app:layout_constraintStart_toStartOf="@+id/connectbutt3"
        app:layout_constraintTop_toTopOf="@+id/connectbutt3" />

    <TextView
        android:id="@+id/ezkart"
        android:layout_width="89dp"
        android:layout_height="33dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
